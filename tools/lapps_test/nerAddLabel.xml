<tool id="test.nerfix" name="Named Entity Normalizer" version="1.0.0">
  <description>Maps NE annotations</description>
  <command interpreter="lsd">nerAddLabel.lsd $input $output</command>
  <inputs>
    <param name="input" type="data" format="lif"/>
  </inputs>
  <outputs>
    <data name="output" format="lif"/>
  </outputs>
  <help>Adds missing Named Entity labels.</help>
</tool>
