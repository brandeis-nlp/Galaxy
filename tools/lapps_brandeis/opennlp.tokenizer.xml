<tool id='opennlp.tokenizer' name='OpenNLP Tokenizer' version='2.0.0'>
  <description>OpenNLP Tokenizer</description>
  <command interpreter='lsd'>invoke.lsd opennlp.OpenNLP $input $output</command>
  <inputs>
    <param name='input' type='data' format='lif' label='input' />
  </inputs>
  <outputs>
    <data name='output' format='lif' />
  </outputs>
</tool>