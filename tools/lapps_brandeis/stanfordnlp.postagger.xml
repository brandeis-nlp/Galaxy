<tool id='stanfordnlp.postagger' name='Stanford POSTagger' version='2.0.0'>
  <description>Stanford CoreNLP POSTagger</description>
  <command interpreter='lsd'>invoke.lsd stanfordnlp.Stanford $input $output</command>
  <inputs>
    <param name='input' type='data' format='lif' label='input' />
  </inputs>
  <outputs>
    <data name='output' format='lif' />
  </outputs>
</tool>