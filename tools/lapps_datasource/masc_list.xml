<tool id="masc.list" name="List MASC" version="2.1.0">
  <description>Lists all keys in MASC.</description>
  <command interpreter="lsd">masc_list.lsd $start $end $output</command>
  <inputs>
  	<param name="start" type="integer" label="Start" value="0" min="0" max="392"/>
  	<param name="end" type="integer" label="End" value="392" min="0" max="392"/>
  </inputs>
  <outputs>
    <data name="output" format="txt"/>
  </outputs>
