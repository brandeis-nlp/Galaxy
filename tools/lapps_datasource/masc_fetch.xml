<tool id="masc.fetch" name="MASC Datasource" version="2.0.0">
  <description>Invokes the Stanford tokenizer service.</description>
  <command interpreter="lsd">fetch.lsd key=${key} token=${token} output=${output}</command>
  <inputs>
    <param name="key" type="text" label="Key" value="MASC3-0203"/>
    <param name="token" type="text" label="OAuth Token" value="123abc"/>
  </inputs>
  <outputs>
    <data name="output" format="json" label="Output"/>
  </outputs>
  <!--
  <tests>
    <test>
      <param name="input" value="/Users/suderman/Projects/Galaxy/text.json"/>
      <output name="output" file="/Users/suderman/Projects/Galaxy/tokens.json"/>
    </test>
  </tests>
  -->
  <help>Fetches a single document from MASC 3.0.0</help>
</tool>
