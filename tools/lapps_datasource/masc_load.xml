<tool id="masc.load" name="MASC Load" version="1.0.0">
  <description>Retrieves a single of document from MASC.</description>
  <command interpreter="lsd">masc_load.lsd $input $index $token $output</command>
  <inputs>
    <!--
    <param name="key" type="text" label="Key" value="MASC3-0203"/>
    -->
    <param name="input" type="data" format="txt"/>
    <param name="index" type="integer" value="0" min="0" max="391"/>
    <param name="token" type="text" label="OAuth Token" value="123abc"/>
  </inputs>
  <outputs>
    <data name="output" format="lif" label="Output"/>
  </outputs>
  <!--
  <tests>
    <test>
      <param name="input" value="/Users/suderman/Projects/Galaxy/text.json"/>
      <output name="output" file="/Users/suderman/Projects/Galaxy/tokens.json"/>
    </test>
  </tests>
  -->
  <help>Retrieves a single document from MASC 3.0.0.</help>
</tool>
